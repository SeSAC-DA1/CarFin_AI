# CarFin AI 환경 변수 설정
# .env.example -> .env 로 복사하여 실제 값 입력

# AWS RDS PostgreSQL 연결 정보
DB_HOST=carfin-db.cbkayiqs4div.ap-northeast-2.rds.amazonaws.com
DB_PORT=5432
DB_NAME=carfin
DB_USER=carfin_user
DB_PASSWORD=your_actual_password_here

# Redis 캐싱 (선택사항)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# Google Cloud AI (선택사항)
GOOGLE_APPLICATION_CREDENTIALS=path/to/service-account.json

# 로깅 레벨
LOG_LEVEL=INFO

# API 키 (개발환경)
API_SECRET_KEY=your_secret_key_here

# WebSocket 설정
WEBSOCKET_PORT=9000
WEBSOCKET_HOST=0.0.0.0